{
  "name": "portfolio",
  "version": "0.0.1",
  "description": "Mike Peritz personal portfolio site.",
  "main": "index.js",
  "scripts": {
    "validate": "check-node-version --node '>= 6.7.0'",
    "setup": "./bin/setup",
    "prep": "npm run validate && npm run setup",
    "postinstall": "npm run prep",
    "build": "webpack",
    "build-watch": "npm run build -- -w",
    "build-dev": "cross-env NODE_ENV=development npm run build-watch",
    "build-branch": "bin/build-branch.sh",
    "start": "node server/start.js",
    "start-watch": "nodemon server/start.js --watch server --watch db",
    "start-dev": "cross-env NODE_ENV=development npm run start-watch",
    "lint": "esw --ext .js,.jsx --ignore-path .gitignore --cache --format node_modules/eslint-formatter-pretty",
    "lint-watch": "npm run lint -- --watch",
    "test": "mocha --compilers js:babel-register app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "test-watch": "npm run test -- --watch",
    "test-dev": "npm run test-watch -- -R mocha-tldr-reporter",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others --prefix \"[{name}]\" --names \"BUILD,SERVE \" -c \"bgBlue.black,bgGreen.black\" \"npm run build-dev\" \"npm run start-dev\"",
    "seed": "node db/seed.js",
    "deploy-heroku": "bin/deploy-heroku.sh"
  },
  "author": "Mike Peritz <github.com/mperitz>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-stage-2": "^6.18.0",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "check-node-version": "^1.1.2",
    "concurrently": "^3.1.0",
    "cookie-session": "^2.0.0-alpha.1",
    "express": "^4.14.0",
    "finalhandler": "^1.0.0",
    "material-ui": "^0.17.2",
    "pretty-error": "^2.0.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-modal": "^1.7.3",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-eslint": "6",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.16.1",
    "eslint-config-fullstack": "^2.7.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "nodemon": "^1.11.0",
    "volleyball": "^1.4.1",
    "webpack": "^2.2.1",
    "webpack-livereload-plugin": "^0.10.0"
  }
}
